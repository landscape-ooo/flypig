.ind_focus {
    height: 210px;
    overflow: hidden;
    position: relative;
    width: 640px;
}
.ind_focus .slides_container {
    height: 210px;
    width: 640px;
}
.ind_focus .pagination {
    bottom: 15px;
    right: 0px;
    position: absolute;
    z-index: 6;
}
.ind_qyjnqyer {
    border-bottom: 8px solid #F5F4F0;
    margin-top: 30px;
    padding-bottom: 30px;
    width: 100%;
}

.pagination {
	margin:26px auto 0;
	width:100px;
}

.pagination li {
	float:left;
	margin:0 1px;
	list-style:none;
}

.pagination li a {
	display:block;
	width:12px;
	height:0;
	padding-top:12px;
	background-image:url(pagination.png);
	background-position:0 0;
	float:left;
	overflow:hidden;
}

.pagination li.current a {
	background-position:0 -12px;
}

.caption {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0.5);
    color: #FFFFFF;
    font-size: 14px;
    left: 0;
    position: absolute;
    text-shadow: none;
    width: 640px;
    z-index: 500;
}
.caption p{margin:0px;padding:10px;}