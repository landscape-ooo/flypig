.btn-ok{background:#95a214;color:#fff;}
.btn-ok:link,.btn-ok:visited{color:#ffffff;background:#a6b21e;}
.btn-ok:hover,.btn-ok:active{color:#f5f9b1;background:#ced83e;}

.btn-success{background:#95a214;color:#fff;}
.btn-success:link,.btn-success:visited{color:#ffffff;background:#a6b21e;}
.btn-success:hover,.btn-success:active{color:#f5f9b1;background:#ced83e;}

.btn-large{padding:0 20px}
.btn-mini{font-size:12px;line-height:14px;padding:5px 10px}

.btn3d{display:inline-block;height:38px;line-height:38px;text-align:center;font-family:'\5FAE\8F6F\96C5\9ED1','Microsoft YaHei';border-radius:6px;box-shadow:0 2px 0 #bbbbbb;}
.btn3d:link,.btn3d:visited{color:#ffffff;background:#a6b21e;}
.btn3d:hover,.btn3d:active{color:#f5f9b1;background:#ced83e;}
.btn3d-full{width:100%;}
.btn3d-two{width:48%;}

.btn-more{display:inline-block;border-radius:5px;font-size:14px;padding:0 8px;}
.btn-more:link,.btn-more:visited{color:#ffffff;background:#b5bf44;}
.btn-more:hover,.btn-more:active{color:#f5f9b1;background:#ced83e;}

.btn-tool{display:inline-block;height:20px;line-height:20px;font-size:12px;padding:0 12px;border-radius:4px;}
.btn-tool:link,.btn-tool:visited{color:#ffffff;background:#0076ca;}
.btn-tool:hover,.btn-tool:active{color:#b3ddf9;background:#0076ca;}

.btn-tool2{display:inline-block;height:20px;line-height:20px;font-size:12px;padding:0 12px;border-radius:4px;}
.btn-tool2:link,.btn-tool2:visited{color:#ffffff;background:#cccccc;}
.btn-tool2:hover,.btn-tool2:active{color:#e2e2e2;background:#cccccc;}

/* tag */
.tag-cates{}
.tag-cates .tag-cate{display:inline-block;height:22px;line-height:22px;font-size:12px;padding:0 12px;border-radius:15px;}
.tag-cates .tag-cate:link,.tag-cates .tag-cate:visited{color:#ffffff;background:#cccccc;}
.tag-cates .tag-cate:hover,.tag-cates .tag-cate:active{color:#ffffff;background:#bec61f;}
.tag-cates .selected:link,.tag-cates .selected:visited{color:#ffffff;background:#bec61f;}
.tag-cates .selected:hover,.tag-cates .selected:active{color:#ffffff;}

/* 通用评论 */
.comment{}
.comment li {margin: 0 0 20px 0;overflow: hidden;}
.comment li .show-info .meta{font-size:13px;padding:5px;margin:0 0 15px 0;background:#f5f5f5;color:#999;display: block;overflow: hidden;position: relative;border-bottom: solid 1px #dedede;}
.comment li .show-info .meta i {position: absolute;top: 5px;right: 5px;}
.comment li .show-info .content p{margin: 0;overflow: hidden;word-wrap: break-word;line-height: 23px;}
.comment li .show-info .recomment {background: #f8f8f8;border: 1px dashed #ddd;color: #666;font-size: 12px;padding: 10px;}
.comment li .group_banned {text-align: right;}

.comment{}
.comment li {margin: 0 0 20px 0;overflow: hidden;}
.comment li .user-face {float: left;height: 48px;overflow: hidden;width: 48px;}
.comment li .reply-doc {margin-left: 60px;overflow: hidden;}
.comment li .reply-doc h4 {font-weight:normal;font-size:13px;padding:5px;margin:0 0 15px 0;background:#f5f5f5;color:#999;display: block;overflow: hidden;position: relative;border-bottom: solid 1px #dedede;}
.comment li .reply-doc h4 i {position: absolute;top: 5px;right: 5px;}
.comment li .reply-doc p {margin: 0;overflow: hidden;word-wrap: break-word;line-height: 23px;}
.comment li .reply-doc .recomment {background: #f8f8f8;border: 1px dashed #ddd;color: #666;font-size: 12px;padding: 10px;}
.comment li .group_banned {text-align: right;}

/*!commentform*/
.commentform{overflow:hidden;}
.commentform .authcode{float:left;}
.commentform .submit{margin-top:10px;float:right;}

/* 评论通用提示 */
.commentmsg{text-align:center;padding:20px;color:#999999;}
i.tip{font-size:11px;font-style:normal;color:#999999;}


#com-tsnotice{padding:80px 0;text-align:center;}
#com-tsnotice p{font-size:14px;line-height:38px;}
#com-tsnotice .tolink{font-size:12px;line-height:24px;}
#com-tsnotice .tolink a{color:#919f0a;}

table tr.old td{font-weight:bold;background:none repeat scroll 0 0 #F6F6F6;}
table tr.odd td{background:none repeat scroll 0 0 #FBFBFB;}

