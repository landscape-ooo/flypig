.btn{display:inline-block;line-height:24px;padding:0 10px;background:#f5f5f5;color:#333;cursor:pointer;text-align:center;vertical-align:middle;}
.btn-success{background:#95a214;color:#fff;}
.btn-success:hover{color:#c7ff8e;text-decoration:none;}
.btn-large{font-size:16px;line-height:normal;padding:9px 20px}
.btn-mini{font-size:12px;line-height:14px;padding:5px 10px}
