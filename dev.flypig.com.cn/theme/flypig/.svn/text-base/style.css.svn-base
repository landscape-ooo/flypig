.midder {
	width: 960px;
	margin: 0 auto;
	padding: 0;
	overflow: hidden;
	height: auto!important;
	min-height: 400px;
	margin-top: 10px
}

.midder .mc {
	overflow: hidden;
	display: block
}

.midder .mc .cleft {
	float: left;
	width: 640px
}

.midder .mc .cright {
	float: right;
	width: 310px
}
.bbox {
	border: solid 1px #ddd;
	margin-bottom: 10px;
	overflow: hidden;
	background: #fff;
}

.bbox .bd {
	font-size: 12px;
	padding: 10px;
	border-bottom: 1px solid #eee
}

.bbox h2 {
	font-weight: bold;
	font-size: 12px;
	border-bottom: solid 1px #eee;
	background: #f5f5f5;
	padding: 8px 5px;
	margin: 0;
	position: relative
}

.bbox h2 .right {
	font-size: 12px;
	font-weight: normal;
	position: absolute;
	top: 5px;
	right: 10px
}

.bbox .bc {
	padding: 10px;
	overflow: hidden;
	clear: both
}

